{
    "manifest_version": 3,
    "name": "Limpador de Números",
    "version": "1.0",
    "description": "Remove espaços e símbolos, deixando apenas números.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["clipboardWrite"]
  }
