{
  "manifest_version": 3,
  "name": "Limpador de Números",
  "description": "Remove caracteres não numéricos automaticamente",
  "version": "2.0",
  "author": "Theo - andersontheoo@gmail.com",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "clipboardWrite"
  ]
}